me":"preview_url","generated":{"line":127,"column":146},"source":"Components/Tracks.js","original":{"line":48,"column":56}},{"generated":{"line":127,"column":157},"source":"Components/Tracks.js","original":{"line":48,"column":37}},{"generated":{"line":127,"column":158},"source":"Components/Tracks.js","original":{"line":48,"column":32}},{"generated":{"line":127,"column":159},"source":"Components/Tracks.js","original":{"line":45,"column":28}},{"generated":{"line":127,"column":160},"source":"Components/Tracks.js","original":{"line":44,"column":24}},{"generated":{"line":128,"column":0},"source":"Components/Tracks.js","original":{"line":51,"column":21}},{"generated":{"line":128,"column":7},"source":"Components/Tracks.js","original":{"line":41,"column":20}},{"generated":{"line":128,"column":8},"source":"Components/Tracks.js","original":{"line":39,"column":12}},{"generated":{"line":128,"column":9},"source":"Components/Tracks.js","original":{"line":38,"column":8}},{"generated":{"line":129,"column":0},"source":"Components/Tracks.js","original":{"line":55,"column":5}},{"name":"Component","generated":{"line":133,"column":2},"source":"Components/Tracks.js","original":{"line":4,"column":21}},{"name":"Tracks","generated":{"line":135,"column":15},"source":"Components/Tracks.js","original":{"line":60,"column":15}}],"sources":{"Components/Tracks.js":"import React, { Component } from 'react';\nimport { render } from 'react-dom';\n\nclass Tracks extends Component {\n    state = ({ playing: false, playingAudio: null, previewNotAvailable: false });\n\n    // Making playAudio function return a function itself by using double arrows. This is done because in the JSX below, the onClick \n    // handler need to have a reference to a function, not call the function witht the paramtere right in the JSX.  \n    playAudio = (previewUrl) => () => { \n        const audio = new Audio( previewUrl );\n\n        if ( previewUrl == null ) {\n            alert('Sorry, this track is not avaiable for preview.');\n        } else if ( !this.state.playing ) {\n            audio.play();\n            this.setState({ playing: true, playingAudio: audio });\n        } else if ( this.state.paused ) {\n            this.state.pausedAudio.play();\n        } else if ( this.state.playingAudio.src !== audio.src) {\n            this.state.playingAudio.pause();\n            audio.play();\n            this.setState({ playingAudio: audio });\n        } else {\n            this.state.playingAudio.pause();    \n            this.setState({ playing: false });\n        }\n    }\n\n    previewButton = (previewUrl) => {\n        if ( previewUrl && this.state.playing && this.state.playingAudio.src === previewUrl) {\n            return ( <span className='track-icon' >&#9724;</span>)\n        } else if ( previewUrl ) {\n            return ( <span className='track-icon' >&#9658;</span>)\n        }\n    }\n\n    render() {\n        return(\n            <div>\n                {\n                    this.props.tracks.map((track) => {\n                        const { id, name, preview_url } = track; \n                        \n                        return (\n                            <div key={id} onClick={ this.playAudio(preview_url) } alt='track-image' className='track'>\n                                <img src={ track.album.images[0].url } className='track-image'></img>\n                                <p>{ name }</p>\n                                <p>{ this.previewButton(preview_url) }</p>\n                            </div>\n                        )\n                    })\n                }\n            </div>\n        )\n    }\n    \n\n}\n\nexport default Tracks;"},"lineCount":null}},"hash":"74065e135b183d1e73d733258cc8ca47","cacheData":{"env":{}}}